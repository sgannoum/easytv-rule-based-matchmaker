<?xml version="1.0"?>
<rdf:RDF
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns="http://www.owl-ontologies.com/OntologyEasyTV.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://www.owl-ontologies.com/OntologyEasyTV.owl">
  <owl:Ontology rdf:about=""/>
  <rdfs:Class rdf:ID="User"/>
  <rdfs:Class rdf:ID="UserPreferences"/>
  <rdfs:Class rdf:ID="VisualAbility"/>
  <rdfs:Class rdf:ID="AuditoryAbility"/>
  <owl:ObjectProperty rdf:ID="hasAuditoryAbility">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#AuditoryAbility"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasVisualAbility">
    <rdfs:range rdf:resource="#VisualAbility"/>
    <rdfs:domain rdf:resource="#User"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPreferences">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#UserPreferences"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="hasHalfK">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#AuditoryAbility"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasFontColor">
    <rdfs:domain rdf:resource="#UserPreferences"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasAudioVolume">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#UserPreferences"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasFonts">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#UserPreferences"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasFourK">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#AuditoryAbility"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasGender">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Define the gender of a user e.g. male or female</rdfs:comment>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Female</rdf:first>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Male</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasVisualAcuity">
    <rdfs:domain rdf:resource="#VisualAbility"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasTwoK">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#AuditoryAbility"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasColorBllindness">
    <rdfs:domain rdf:resource="#VisualAbility"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasOneK">
    <rdfs:domain rdf:resource="#AuditoryAbility"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasEightK">
    <rdfs:domain rdf:resource="#AuditoryAbility"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasLanguageAudio">
    <rdfs:domain rdf:resource="#UserPreferences"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasLanguageSubtitles">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#UserPreferences"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasAge">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Property that defines the age of a user</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasBackgroundColor">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#UserPreferences"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasQuarterQ">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#AuditoryAbility"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasLanguageSign">
    <rdfs:domain rdf:resource="#UserPreferences"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasContrastSensitivity">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#VisualAbility"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasContrast">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#UserPreferences"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasFontSize">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#UserPreferences"/>
  </owl:DatatypeProperty>
</rdf:RDF>

